<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="poco.cn.jetpackdemo">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".singletest.liveData.LiveDataActivity" />
        <activity android:name=".singletest.lifecycle.LifecycleActivity" />
        <activity
            android:name=".audioPaging.AudioPagingActivity"
            android:label="@string/title_activity_audio_paging"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".samplePaging.proxy.HeaderProxyActivity"
            android:label="@string/title_activity_header_proxy"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".samplePaging.basic.BasicUsageActivity"
            android:label="@string/title_activity_basic_usage"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>